		<script type="text/javascript">


               
			function vote(voteVal, obId){
				
				$.ajax({
					url: "http://ww.profilegraveyard.com/post/vote" + "/" + obId + "/" + voteVal,
					type: 'POST',
					success: function(data){
						$('#' + obId).html(data);
					}
				});
				return false;
			}
			var togval = 0;
			function togglesubmit()
			{
				if (togval == 0)
				{
					$('#newObit').slideDown(1000);
					togval = 1;
				} else {
					$('#newObit').slideUp(1000);
					togval = 0;
				}
				return false;
			}
			
			function deathdate()
			{
				$('#deathy')
				    .find('option')
				    .remove()
				    .end()
				    .append('<option value="0">Died</option>')
				    .val('0')
				;
				var ddl = $('#deathy');
				for (i = $('#birthy').val(); i <= 2011; i++)
				{
					ddl.append("<option value='" +i+ "'>" + i + "</option>");
				}
			}

	function validate(){
		$.ajax({url: "http://ww.profilegraveyard.com/post/newobit",
			data: $('form').serialize(),type: 'POST',
			success: function(data){$('#xtra').html(data);
				}
		});
		return false;
	}
	
</script>
